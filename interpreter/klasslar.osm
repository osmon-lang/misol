guruh Odam {

    joy ism;
    joy familiya; 

    funksiya yarat(i, f) {
        shu.ism = i;
        shu.familiya = f;
        qaytar shu;
    }

    funksiya korsat() {
        Tizim.yoz(shu.matnga());
    }

    funksiya matnga() {
        qaytar tahrirla("Shu odamning ismi ", shu.ism, " va familiyasi ", shu.familiya, "!");
    }
}

funksiya asosiy() {
    joy demo = Odam("UwUssimo", "Robinson");
    demo.korsat();
}
